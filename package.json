{
  "name": "iconify",
  "description": "Iconify integration. Import MDI, FontAwesome, Jam, EmojiOne and many other icons to Sketch document.",
  "version": "1.0.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "iconify",
    "manifest": "src/manifest.json",
    "main": "iconify.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/iconify-design/iconify-sketch.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build",
    "rel": "NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.15"
  },
  "author": "Vjacheslav Trushkin <cyberalien@gmail.com>",
  "dependencies": {
    "sketch-module-web-view": "^2.1.7"
  }
}
